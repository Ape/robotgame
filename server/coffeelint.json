{
    "colon_assignment_spacing": {
        "level": "error",
        "spacing": {
            "left": 0,
            "right": 1
        }
    },
    "indentation": {
        "value": 1,
        "level": "error"
    },
    "line_endings": {
        "level": "error",
        "value": "unix"
    },
    "no_empty_param_list": {
        "level": "error"
    },
    "no_implicit_parens": {
        "level": "error",
        "strict": true
    },
    "no_interpolation_in_single_quotes": {
        "level": "error"
    },
    "no_tabs": {
        "level": "ignore"
    },
    "no_trailing_whitespace": {
        "level": "error",
        "allowed_in_comments": false,
        "allowed_in_empty_lines": false
    },
    "no_unnecessary_double_quotes": {
        "level": "error"
    },
    "space_operators": {
        "level": "warn"
    }
}
